{
  "id": "processEngineConfiguration",
  "type": "org.activiti.engine.impl.cfg.StandaloneProcessEngineConfiguration",
  "databaseSchemaUpdate": "updated",
  "asyncExecutorDefaultAsyncJobAcquireWaitTime": 1000000,
  "asyncExecutorDefaultTimerJobAcquireWaitTime": 1000000,
  "asyncExecutorThreadKeepAliveTime": 5000,
  "asyncExecutorActivate": true,
  "asyncFailedJobWaitTime": 1,
  "mailServerPort": "5025",
  "mailServers": [
    {
      "myEmailTenant": {
        "type": "org.activiti.engine.cfg.MailServerInfo",
        "mailServerHost": "localhost",
        "mailServerPort": "5025",
        "mailServerUseSSL": "false",
        "mailServerUseTLS": "false",
        "mailServerDefaultFrom": "activiti@myTenant.com",
        "mailServerUsername": "activiti@myTenant.com",
        "mailServerPassword": "password"
      }
    }
  ],
  "bpmnExtensionsNamespace": "http://camunda.org/schema/1.0/bpmn",
  "bpmnExtensionsPrefix": "camunda",
  "history": "full",
  "enableProcessDefinitionInfoCache": true
}
