    <bpmn2:sequenceFlow id="SequenceFlow_0049oal" sourceRef="Task_085qd6k" targetRef="Task_0ruarv8" />
    <bpmn2:userTask id="Task_0ruarv8" name="支付" camunda:formKey="4" camunda:assignee="${name}">
      <bpmn2:incoming>SequenceFlow_0049oal</bpmn2:incoming>
    </bpmn2:userTask>
